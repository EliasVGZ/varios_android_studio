<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center"
    android:layout_margin="12dp">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ejemplos de intents implicitos"
        android:textSize="20dp"/>
    <Button
        android:id="@+id/btn_contactos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mostrar lista de contactos"
        android:onClick="onClickBotones">

    </Button>
    <Button
        android:id="@+id/btn_llamar_dial"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Llamada al dial"
        android:onClick="onClickBotones">

    </Button>
    <Button
        android:id="@+id/btn_llamar_dial_con_numero"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Llamada al dial con numero premarcado"
        android:onClick="onClickBotones">

    </Button>
    <Button
        android:id="@+id/btn_call_directo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Llamada directamente a un numero"
        android:onClick="onClickBotones">

    </Button>


</LinearLayout>